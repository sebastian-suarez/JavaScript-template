{
	"name": "javascript-template",
	"version": "1.0.0",
	"description": "Starter Node.js JavaScript template with ESM, import aliases, Jest, XO, Prettier, and Husky.",
	"keywords": [
		"javascript",
		"nodejs",
		"template",
		"starter",
		"esm",
		"import-alias",
		"jest",
		"xo",
		"prettier",
		"husky"
	],
	"homepage": "https://github.com/sebastian-suarez/JavaScript-template#readme",
	"bugs": {
		"url": "https://github.com/sebastian-suarez/JavaScript-template/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sebastian-suarez/JavaScript-template.git"
	},
	"license": "MIT",
	"author": "Sebastian Suarez <261091557+sebastian-suarez@users.noreply.github.com> (https://www.sebastiansuarez.dev)",
	"type": "module",
	"imports": {
		"#src/*": "./src/*",
		"#modules/*": "./src/modules/*"
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"start": "node ./src/index.js",
		"test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
		"lint": "xo",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"lint-staged": "^16.2.7",
		"prettier": "3.8.1",
		"xo": "^1.2.3"
	}
}
